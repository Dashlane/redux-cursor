{
    "rules": {
        "class-name": true,
        "forin": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-debugger": true,
        "no-duplicate-variable": true,
        "no-eval": true,
        "no-inferrable-types": true,
        "no-switch-case-fall-through": true,
        "no-trailing-whitespace": true,
        "no-unreachable": true,
        "no-unused-expression": true,
        "no-unused-variable": [true, "react"],
        "semicolon": [true, "never"],
        "triple-equals": true
    }
}
